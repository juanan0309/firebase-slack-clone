{
  "extends": ["airbnb", "prettier", "plugin:react/jsx-runtime"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {}, {
      "usePrettierrc": false
    }],
    "import/no-named-as-default": 0,
    "import/no-named-as-default-member": 0,
    "react/jsx-no-useless-fragment": 0,
    "no-unused-expressions": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
